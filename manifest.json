{
	"manifest_version": 2,

	"name": "QR This",
	"description": "Displays a QR code for the current tabs URL.",
	"version": "1.0",
	"icons": { "16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" },
		"browser_action": {
			"default_icon": "icon.png",
			"default_popup": "popup.html"
		},
		"permissions": [
			"activeTab",
			"https://goqr.me/"
		]
}
